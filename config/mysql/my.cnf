[mysqld]
skip-name-resolve
sql_mode=NO_ENGINE_SUBSTITUTION,STRICT_TRANS_TABLES

max_connections=20
table_open_cache=64
thread_cache_size=4

innodb_buffer_pool_size=64M
innodb_log_buffer_size=8M
innodb_log_file_size=48M
innodb_file_per_table=1

key_buffer_size=16M
tmp_table_size=32M
max_heap_table_size=32M

general_log=0
slow_query_log=0
